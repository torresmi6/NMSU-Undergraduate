-- globals for draw
cursor_h = {
    x=12,
    y=108,
    sprite=11,
    clr = 7
}

cursor_v = {
    x=5,
    y=4,
    sprite=12,
    clr = 7
}

--stops the intersection from drawing when true
itrs_done = false

function _draw()

 if(scene == "title") then
 	draw_title()
 elseif(scene == "instr") then
  draw_instr()
 elseif(scene == "game") then
  draw_game()
 elseif(scene == "combat") then
 	draw_combat()
 elseif(scene == "end") then
 	draw_end()
 end
 
end

function draw_combat()
 cls()
 --static map, can add more
 map() 
 draw_enemies()
	draw_cursor_h()
	draw_cursor_v()
	draw_attempts()
	if (combat_h == 1) then
		move_cursor_h()
	end
	if (combat_v == 1) then
		move_cursor_v()
	end
	
	if (cursor_h_dir == 2 and cursor_v_dir == 2) then
		draw_intersection()
		if calc_done == false then
			calc_results()
			if health == 0 then scene = "end"
			end
		end
		draw_result()
		delay(50)
		hits = 0
	end
end

function draw_cursor_h()
	spr(cursor_h.sprite, cursor_h.x, cursor_h.y)
end

function draw_cursor_v()
	spr(cursor_v.sprite, cursor_v.x, cursor_v.y)
end

function draw_enemies()
	for i = 1, num_enm do
		spr(21, enemies[i][1], enemies[i][2])
	end
end

function draw_intersection()
	if(itrs_done == false) then
		for x = 1, num_enm do
			for i = 1, (usr_atmpts[x][1] - 16) do
				rect(16, usr_atmpts[x][2],16+i, usr_atmpts[x][2], 7+x) 
				--delay(.01)
			end
		
			for a = 1, (103 - usr_atmpts[x][2]) do
		
				rect(usr_atmpts[x][1], 103-a,usr_atmpts[x][1], 103, 7+x) 
				--delay(.01)
			end
		end
		
		itrs_done = true
		else 
			for y = 1, num_enm do
				rect(16, usr_atmpts[y][2], usr_atmpts[y][1], usr_atmpts[y][2],7+y)
				rect(usr_atmpts[y][1], usr_atmpts[y][2],usr_atmpts[y][1], 102,7+y)
			end
		end
end

function draw_attempts()
	for i = 1, atmpt_cnt_x do
		rect(usr_atmpts[i][1],103,usr_atmpts[i][1],104,7+i) 
	end
	
	for i = 1, atmpt_cnt_y do
		rect(15,usr_atmpts[i][2],16,usr_atmpts[i][2],7+i) 
	end
	
end

function draw_result()
	
	if life_lost == 0 then
		print(hits_temp.." hits!  no life lost  lives:"..health,7)
	elseif life_lost > 1 then print(hits_temp.." hits!  "..life_lost.." lives lost  lives:"..health,7)
	else print(hits_temp.." hits!  1 life lost  lives:"..health,7)
	end
	
end

function draw_title()

	cls()
	
	
end

function draw_instr()
 cls()
       
end

function draw_end()
	cls()
	spr(71,23,20)
	delay(1)
	spr(72,32,20)
	delay(1)
	spr(73,41,20)
	delay(1)
	spr(74,50,20)
	delay(1) 
	spr(75,67,20)
	delay(1)
	spr(76,76,20)
	delay(1)
	spr(74,85,20)
	delay(1)
	spr(77,94,20)
	delay(3)
	print("press ‚ùé to retry", 30, 90, 7)
	print("press üÖæÔ∏è to return to title", 10, 110, 7)
	scene = "end_idle"
end

