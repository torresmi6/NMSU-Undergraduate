-- note: function update every
-- 60 fps

-- globals for update

--cursor directions
--	if 0, cursor moves right/down
-- if 1, cursor moves left/up
-- if 2, cursor stops moving
cursor_h_dir = 0
cursor_v_dir = 0

--combat state
-- if 0, not current
-- if 1, current state
combat_h = 1
combat_v = 0

--attempts set based on enemies
-- cnt used to keep track of btn presses
attempts = 1
atmpt_cnt_x = 0
atmpt_cnt_y = 0

--array of user attempts
usr_atmpts = {}

--keeps track of successful hits
hits = 0
--keeps track of previous hits for printing
hits_temp = 0
--when true, stops calculating
calc_done = false
function _update60()

 if(scene == "title" or
    scene == "title_idle") then
  if(scene == "title") then
   sfx(1)
  end
	 title_controls()
	elseif(scene == "instr") then
	 instr_controls()
	elseif(scene == "game") then
	 game_controls()
	elseif(scene == "combat") then
		combat_controls() 
	elseif(scene == "end_idle") then
		end_controls()
	end

end

function game_controls()

end

function end_controls()
	if btnp(‚ùé) then
		reset_combat()
		scene = "combat"
	elseif btnp(üÖæÔ∏è) then
		scene = "title"
		--sfx(1)
	end
end

function title_controls()
	if btnp(‚ùé) then
		scene = "instr"
	end
end

function instr_controls()
	if btnp(‚ùé) then
		scene = "game"
		--music(0)
	end
end

function combat_controls()

	if (combat_v == 1) then
		
		if atmpt_cnt_y == attempts then
			cursor_v_dir = 2
			combat_v = 0
		end
		
		if (btnp(üÖæÔ∏è,0)) then
			atmpt_cnt_y += 1
			usr_atmpts[atmpt_cnt_y][2] = cursor_v.y+4
			cursor_v.clr += 1
			cursor_v.sprite +=2
			if cursor_v.sprite == 22 then cursor_v.sprite = 12 end
		end
	end


 if (combat_h == 1) then
		if atmpt_cnt_x == attempts then
			cursor_h_dir = 2
			combat_h = 0
			combat_v = 1
		end
		
		if (btnp(üÖæÔ∏è,0)) then
			atmpt_cnt_x += 1
			usr_atmpts[atmpt_cnt_x][1] = cursor_h.x+4
		 cursor_h.clr += 1
		 cursor_h.sprite +=2
		 if cursor_h.sprite == 21 then cursor_h.sprite = 11 end 
		end
	end
end

function move_cursor_h()


	if(cursor_h.x == 114) then
		cursor_h_dir = 1
	end
	
	if(cursor_h.x == 13) then
		cursor_h_dir = 0
	end
	
	if(cursor_h_dir == 0) then
		cursor_h.x += 1
	elseif (cursor_h_dir == 1) then
	 cursor_h.x -= 1
	end
	
end

function move_cursor_v()

	if(cursor_v.y == 98) then
		cursor_v_dir = 1
	end
	
	if(cursor_v.y == 4) then
		cursor_v_dir = 0
	end
	
	if(cursor_v_dir == 0) then
		cursor_v.y += 1
	elseif (cursor_v_dir == 1) then
	 cursor_v.y -= 1
	end
	
end

function calc_results()
	for i = 1, num_enm do
		for a = 1, num_enm do
			if (usr_atmpts[a][1] - enemies[i][1]) < 8 and (usr_atmpts[a][1] - enemies[i][1]) > 0 then
				if (usr_atmpts[a][2] - enemies[i][2]) < 8 and (usr_atmpts[a][2] - enemies[i][2]) > 0 then
				hits += 1
				end
			end	
		end
	end
	
	life_lost = num_enm - hits
	health = health - life_lost
	if health < 0 then health = 0
	end
	hits_temp = hits
	calc_done = true
end

function reset_combat()
	cursor_h_dir = 0
	cursor_v_dir = 0
	combat_h = 1
	combat_v = 0
	attempts = 1
	atmpt_cnt_x = 0
	atmpt_cnt_y = 0
	calc_done = false
	health = 5
	
	cursor_h = {
    x=12,
    y=108,
    sprite=11,
    clr = 7
	}

	cursor_v = {
    x=5,
    y=4,
    sprite=12,
    clr = 7
	}
	itrs_done = false

	set_enemies()
end
