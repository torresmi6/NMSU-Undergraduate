	@Override
	public boolean testGoal(char[] state) {
		if(state[1] == '0' && state[2] == '0' && state[3] == '0') {
			return true;
		}
		return false;
	}

	@Override
	public double getStepCosts(char[] state, char[] action, char[] stateDelta) {
		return 1;
	}

